{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      },
      "HttpsInlineCertAndKeyFile": {
        "Url": "https://+:443",
        "Certificate": {
          "Path": "/https/cert.crt",
          "KeyPath": "/https/cert.key"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}